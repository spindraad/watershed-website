{
  "name": "watershed-website",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "build": "run-p build:*",
    "build:remix": "remix vite:build",
    "build:storybook": "storybook build",
    "dev": "run-p dev:*",
    "dev:remix": "remix vite:dev",
    "dev:storybook": "storybook dev --no-open -p 6006",
    "test": "run-p test:unit test:components",
    "test:unit": "vitest",
    "test:unit:watch": "vitest --watch",
    "test:unit:coverage": "vitest --coverage",
    "test:components": "test-storybook",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:staged": "lint-staged",
    "format": "prettier --write .",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "prepare": "husky",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push && prisma generate",
    "db:seed": "prisma db seed",
    "db:reset": "prisma migrate reset",
    "db:studio": "prisma studio --browser none"
  },
  "dependencies": {
    "@prisma/client": "^5.21.0",
    "@remix-run/node": "^2.13.1",
    "@remix-run/react": "^2.13.1",
    "@remix-run/serve": "^2.13.1",
    "@sendgrid/mail": "^8.1.3",
    "@shoelace-style/shoelace": "^2.17.1",
    "fs-extra": "^11.2.0",
    "isbot": "^5.1.17",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.0",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.12.0",
    "@remix-run/dev": "^2.13.1",
    "@storybook/addon-a11y": "^8.3.5",
    "@storybook/addon-essentials": "^8.3.5",
    "@storybook/addon-interactions": "^8.3.5",
    "@storybook/addon-links": "^8.3.5",
    "@storybook/addon-onboarding": "^8.3.5",
    "@storybook/addon-viewport": "^8.3.5",
    "@storybook/blocks": "^8.3.5",
    "@storybook/experimental-addon-test": "^8.3.5",
    "@storybook/react": "^8.3.5",
    "@storybook/react-vite": "^8.3.5",
    "@storybook/test": "^8.3.5",
    "@storybook/test-runner": "^0.19.1",
    "@types/node": "^20.14.15",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^8.9.0",
    "@typescript-eslint/parser": "^8.9.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.12.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-storybook": "^0.9.0",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prisma": "^5.21.0",
    "storybook": "^8.3.5",
    "tailwindcss": "^3.4.14",
    "tailwindcss-plugin-custom-elements": "^1.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint -- --fix",
      "npm run format --"
    ]
  },
  "prisma": {
    "seed": "node --loader ts-node/esm prisma/seed.ts"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
